<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/white"/>
            <stroke
                android:width="@dimen/oneDp"
                android:color="@color/grey"/>
            <corners
                android:radius="@dimen/fourDp"
                />
        </shape>
    </item>
    <item
        android:left="@dimen/eightDp"
        android:right="@dimen/eightDp"
        android:top="@dimen/eightDp"
        android:bottom="@dimen/eightDp">
        <shape

            android:shape="rectangle">
            <solid
                android:color="#F1F1F2"/>
            <stroke
                android:dashGap="4dp"
                android:dashWidth="2dp"
                android:width="@dimen/oneDp"
                android:color="#A6A8AB"/>
            <corners
                android:radius="@dimen/fourDp"
                />
        </shape>
    </item>
</layer-list>



