<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="@dimen/eightyDp">
    <View
        android:layout_width="@dimen/oneDp"
        android:layout_height="@dimen/twentyFourDp"
        android:background="@color/black"
        android:id="@+id/v1"

        android:layout_marginLeft="24dp"/>
    <View
        android:layout_width="@dimen/oneDp"
        android:layout_height="match_parent"
        android:background="@color/black"
        android:id="@+id/v2"

        android:layout_marginTop="@dimen/twentyFourDp"
        android:layout_marginLeft="24dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/circle_done"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/fourteenSp"
       android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/sixtyDp"
        android:text="vefdvdfvd"
        android:id="@+id/tvStatus"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/scheduler"
        android:layout_marginLeft="@dimen/sixtyDp"
        android:drawablePadding="@dimen/eightDp"
        android:layout_below="@+id/tvStatus"
        android:layout_marginTop="@dimen/eightDp"
        android:textSize="@dimen/fourteenSp"
        android:text="vfkmvkfvk"

        android:id="@+id/tvDate"/>


</RelativeLayout>