<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/sixtyDp"
        android:background="@color/notifycolor">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_back"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/fourDp"
            android:padding="@dimen/eightDp"
            android:id="@+id/ivStaticWebBack"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textSize="@dimen/eightenSp"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:id="@+id/tvWebHeading"/>
    </RelativeLayout>
<WebView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/wbStaticView"></WebView>
</LinearLayout>